{
	"name": "Surviv.io Cheat Injector",
	"version": "1.1.3",
	"description": "Provides an assortment of cheats for surviv.io",
	"manifest_version": 2,
	"permissions": ["webRequest", "*://*/*", "webRequestBlocking", "tabs", "declarativeContent", "activeTab"],
	"externally_connectable": {
		"matches": ["*://surviv.io/*", "*://vikingpress.online/proxy/*/surviv.io/*"]
	},
	"icons": {
		"128": "./files/images/ice.png"
	},
	"background": {
		"scripts": ["background.js"]
	},
	"web_accessible_resources": ["files/*"],
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
	"content_scripts": [
		{
			"matches": ["*://surviv.io/*", "*://vikingpress.online/proxy/*/surviv.io/*"],
			"js": ["content.js"]
		}
	]
}
